<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.cmcc.mobilehealthcare.activity.MainActivity">
    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        >
    </FrameLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/tab_bg"
        android:orientation="horizontal" >
        <TextView
            android:id="@+id/aurora"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp"
            android:text="极光"
            android:textSize="11sp"
            android:textColor="@drawable/text_color_selector"
            android:drawableTop="@drawable/aurora_bg_selector"
            android:gravity="center"
            />
        <TextView
            android:id="@+id/sell"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp"
            android:text="营销"
            android:textSize="11sp"
            android:textColor="@drawable/text_color_selector"
            android:drawableTop="@drawable/sell_bg_selector"
            android:gravity="center"
            />
        <TextView
            android:id="@+id/install"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp"
            android:text="安装"
            android:textSize="11sp"
            android:textColor="@drawable/text_color_selector"
            android:drawableTop="@drawable/install_bg_selector"
            android:gravity="center"
            />
        <TextView
            android:id="@+id/serve"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp"
            android:text="服务"
            android:textSize="11sp"
            android:textColor="@drawable/text_color_selector"
            android:drawableTop="@drawable/server_bg_selector"
            android:gravity="center"
            />
        <TextView
            android:id="@+id/me"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp"
            android:text="我的"
            android:textSize="11sp"
            android:textColor="@drawable/text_color_selector"
            android:drawableTop="@drawable/me_bg_selector"
            android:gravity="center"
            />
    </LinearLayout>
</LinearLayout>
